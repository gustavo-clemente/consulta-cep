{
    "name": "gustavo/consulta-cep",
    "description": "Uma simples biblioteca escrita em PHP para consulta de CEP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gustavo\\ConsultaCep\\": "src/"
        }
    },
    "authors": [
        {
            "name": "gustavo clemente",
            "email": "gustavo.clementelopes@gmail.com"
        }
    ],
    "scripts": {
        "changelog": "conventional-changelog",
        "release": "conventional-changelog --commit",
        "release:patch": "conventional-changelog --patch --commit",
        "release:minor": "conventional-changelog --minor --commit",
        "release:major": "conventional-changelog --major --commit",
        "release:first": "conventional-changelog --first-release --commit",
        "test" : "phpunit test/",
        "test:log" : "phpunit test/ --testdox-text=test_log.txt",
        "test:coverage": "phpunit test/ --coverage-html=_coverage --whitelist=src/"
    },
    "require": {
        "php": ">=7.4.0"
    },
    "require-dev": {
        "marcocesarato/php-conventional-changelog": "^1.13",
        "phpunit/phpunit": "^9.5"
    },
    "version": "1.1.0"
}
